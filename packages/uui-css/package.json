{
  "name": "@umbraco-ui/uui-css",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Css"
  ],
  "description": "Umbraco UI css component",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-css"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/uui-css.min.js",
  "module": "./lib/index.js",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "lit": "^2.0.0"
  },
  "scripts": {
    "build": "npm run build:css && npm run custom-properties-cache && tsc --build --force && rollup -c rollup.config.js",
    "build:css": "postcss lib/custom-properties.css --base lib/ --dir dist && postcss lib/local-typography.css --base lib/ --dir dist && postcss lib/root.css --base lib/ --dir dist",
    "clean": "tsc --build --clean && rimraf dist lib/*.js lib/**/*.js custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json",
    "custom-properties-cache": "node ./scripts/update-custom-properties-cache.mjs"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://uui.umbraco.com/?path=/story/uui-css"
}
