{
  "name": "@umbraco-ui/uui-loader-bar",
  "version": "0.0.5",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Loader"
  ],
  "description": "A loader element in form of bar. Can show progress",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-loader-bar"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "^0.0.5"
  },
  "scripts": {
    "build": "tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  }
}