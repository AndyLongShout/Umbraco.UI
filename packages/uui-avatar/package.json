{
  "name": "@umbraco-ui/uui-avatar",
  "version": "0.0.6",
  "license": "MIT",
  "description": "An avatar web component for displaying user avatars.",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Avatar"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-avatar"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/index.js",
  "module": "./lib/index.js",
  "types": "./lib/uui-avatar.element.d.ts",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "^0.0.6"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib/**/*.js lib/**/*.js.map lib/**/*.d.ts",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
