{
  "name": "@umbraco-ui/uui",
  "version": "0.0.1",
  "license": "MIT",
  "description": "The full Umbraco UI Library",
  "main": "./dist/index.js",
  "module": "./lib/index.js",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-avatar": "0.0.9",
    "@umbraco-ui/uui-avatar-group": "0.0.9",
    "@umbraco-ui/uui-badge": "0.0.2",
    "@umbraco-ui/uui-box": "0.0.4",
    "@umbraco-ui/uui-breadcrumbs": "0.0.9",
    "@umbraco-ui/uui-button": "0.1.0",
    "@umbraco-ui/uui-button-group": "0.0.2",
    "@umbraco-ui/uui-checkbox": "0.0.9",
    "@umbraco-ui/uui-input": "0.0.9",
    "@umbraco-ui/uui-keyboard-shortcut": "0.0.3",
    "@umbraco-ui/uui-loader": "0.0.9",
    "@umbraco-ui/uui-loader-bar": "0.0.10",
    "@umbraco-ui/uui-loader-circle": "0.0.10",
    "@umbraco-ui/uui-radio": "0.0.10",
    "@umbraco-ui/uui-slider": "0.0.9",
    "@umbraco-ui/uui-table": "0.0.9",
    "@umbraco-ui/uui-tabs": "0.0.9",
    "@umbraco-ui/uui-tag": "0.0.9",
    "@umbraco-ui/uui-toggle": "0.0.9"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib/*.js lib/**/*.js custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  }
}
