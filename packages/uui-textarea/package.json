{
  "name": "@umbraco-ui/uui-textarea",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "Umbraco",
    "Custom elements",
    "Web components",
    "UI",
    "Lit",
    "Textarea"
  ],
  "description": "Umbraco UI textarea component",
  "repository": {
    "type": "git",
    "url": "https://github.com/umbraco/Umbraco.UI.git",
    "directory": "packages/uui-textarea"
  },
  "bugs": {
    "url": "https://github.com/umbraco/Umbraco.UI/issues"
  },
  "main": "./dist/uui-textarea.min.js",
  "module": "./lib/index.js",
  "customElements": "custom-elements.json",
  "files": [
    "dist",
    "lib/**/*.d.ts",
    "lib/**/*.js",
    "custom-elements.json"
  ],
  "dependencies": {
    "@umbraco-ui/uui-base": "0.0.11"
  },
  "scripts": {
    "build": "npm run analyze && tsc --build --force && rollup -c rollup.config.js",
    "clean": "tsc --build --clean && rimraf dist lib/*.js lib/**/*.js custom-elements.json",
    "analyze": "web-component-analyzer **/*.element.ts --outFile custom-elements.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://uui.umbraco.com/?path=/story/uui-textarea"
}